				`unidades-medida`.indiceConversion,
				monedas.cotizacion
				FROM `articulos-proveedores`
				INNER JOIN monedas
				ON `articulos-proveedores`.idMoneda = monedas.id
				INNER JOIN terceros
				ON `terceros`.id = `articulos-proveedores`.idTercero
				LEFT JOIN `unidades-medida`
