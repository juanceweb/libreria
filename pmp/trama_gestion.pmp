  <CustomScript ScriptType="Template" ScriptCodeType="Table" ScriptName="Content" ScriptCtrlID="custom" ScriptL1Key="funciones.php" ScriptL2Key=""><![CDATA[<?php

include_once("connect.php");

include_once("ewshared13.php");


